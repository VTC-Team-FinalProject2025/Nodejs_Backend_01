model File {
  id          Int      @id @default(autoincrement())
  uploaderId  Int
  messageType String
  fileType    String
  messageId   Int?
  field       String
  createdAt   DateTime @default(now())

  User Users @relation("UserFiles", fields: [uploaderId], references: [id], onDelete: Cascade)
  

  @@map("file")
}
